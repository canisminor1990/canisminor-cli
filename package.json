{
  "private": true,
  "name": "canisminor-cli",
  "version": "1.0.0",
  "repository": "git@github.com:canisminor1990/canisminor-cli.git",
  "author": "CanisMinor <i@canisminor.cc>",
  "license": "MIT",
  "bin": "dist/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/index.tsx --files",
    "build": "tsc && chmod +x dist/index.js",
    "pretest": "npm run build",
    "test": "xo && ava",
    "lint": "eslint --fix ./src --ext .ts,.tsx,.js --quiet",
    "ci": "npm run lint && npm run type-check"
  },
  "files": [
    "dist/index.js"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "ink": "^3.2.0",
    "ink-ascii": "^0.0.4",
    "ink-big-text": "^1.2.0",
    "ink-color-pipe": "^3.0.0",
    "ink-confirm-input": "^2.0.0",
    "ink-divider": "^3.0.0",
    "ink-form": "^1.0.1",
    "ink-gradient": "^2.0.0",
    "ink-image": "^2.0.0",
    "ink-link": "^2.0.0",
    "ink-markdown": "^1.0.1",
    "ink-multi-select": "^2.0.0",
    "ink-progress-bar": "^3.0.0",
    "ink-quicksearch-input": "^1.0.0",
    "ink-select-input": "^4.2.1",
    "ink-spinner": "^4.0.3",
    "ink-syntax-highlight": "^1.0.1",
    "ink-tab": "^4.2.0",
    "ink-table": "^3.0.0",
    "ink-task-list": "^1.1.0",
    "ink-testing-library": "^2.1.0",
    "ink-text-input": "^4.0.3",
    "ink-use-stdout-dimensions": "^1.0.5",
    "meow": "^9.0.0",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^2.0.0",
    "@types/chalk": "^2.2.0",
    "@types/ink": "^2.0.3",
    "@types/ink-big-text": "^1.2.1",
    "@types/ink-divider": "^2.0.2",
    "@types/ink-gradient": "^2.0.1",
    "@types/ink-select-input": "^3.0.1",
    "@types/ink-spinner": "^3.0.1",
    "@types/ink-table": "^1.0.3",
    "@types/ink-text-input": "^2.0.2",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.4.4"
  }
}
