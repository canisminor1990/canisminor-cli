{
  "name": "@canisminor/cli",
  "version": "1.3.0",
  "description": "ðŸ§¼ CLI tools for fun",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canisminor1990/canisminor-cli.git"
  },
  "bugs": {
    "url": "https://github.com/canisminor1990/canisminor-cli/issues"
  },
  "homepage": "https://github.com/canisminor1990/canisminor-cli#readme",
  "author": "CanisMinor <i@canisminor.cc>",
  "license": "MIT",
  "bin": {
    "cmli": "dist/index.js"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "webpack-cli --config ./config/webpack.common.js --watch",
    "build": "webpack-cli --config ./config/webpack.prod.js && chmod +x dist/index.js",
    "---------": "",
    "pretest": "yarn build",
    "test": "npm run lint",
    "---------  ": "",
    "release": "semantic-release",
    "---------    ": "",
    "lint": "eslint --cache --quiet ./src --ext .ts,.tsx,.js --format=pretty && yarn type-check",
    "lint:fix": "eslint --fix --cache --quiet --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint-staged": "lint-staged",
    "type-check": "tsc --noEmit",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "---------   ": "",
    "clean": "rm -rf dist",
    "reinstall": "yarn clean && rm -rf node_modules && yarn"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write",
      "yarn lint:fix"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "ink": "^3.2.0",
    "ink-ascii": "^0.0.4",
    "ink-big-text": "^1.2.0",
    "ink-confirm-input": "^2.0.0",
    "ink-divider": "^3.0.0",
    "ink-form": "^1.0.1",
    "ink-gradient": "^2.0.0",
    "ink-image": "^2.0.0",
    "ink-link": "^2.0.0",
    "ink-multi-select": "^2.0.0",
    "ink-progress-bar": "^3.0.0",
    "ink-select-input": "^4.2.1",
    "ink-spinner": "^4.0.3",
    "ink-syntax-highlight": "^1.0.1",
    "ink-tab": "^4.2.0",
    "ink-table": "^3.0.0",
    "ink-task-list": "^1.1.0",
    "ink-testing-library": "^2.1.0",
    "ink-text-input": "^4.0.3",
    "ink-use-stdout-dimensions": "^1.0.5",
    "meow": "^9.0.0",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@radix-ui/colors": "^0.1.8",
    "@sindresorhus/tsconfig": "^2.0.0",
    "@types/chalk": "^2.2.0",
    "@types/chroma-js": "^2.1.3",
    "@types/fs-extra": "^9.0.13",
    "@types/ink": "^2.0.3",
    "@types/ink-big-text": "^1.2.1",
    "@types/ink-divider": "^2.0.2",
    "@types/ink-gradient": "^2.0.1",
    "@types/ink-select-input": "^3.0.1",
    "@types/ink-spinner": "^3.0.1",
    "@types/ink-table": "^1.0.3",
    "@types/ink-text-input": "^2.0.2",
    "@types/lodash": "^4.14.181",
    "@types/meow": "^6.0.0",
    "@types/node": "^17.0.23",
    "@types/plist": "^3.0.2",
    "@types/react": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "cache-loader": "^4.1.0",
    "chroma-js": "^2.4.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitlint": "^16.2.3",
    "commitlint-config-gitmoji": "^2.2.5",
    "copy-webpack-plugin": "^10.2.4",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "fs-extra": "^10.0.1",
    "husky": "^4.3.8",
    "ignore-loader": "^0.1.2",
    "lint-staged": "^12.3.7",
    "plist": "^3.0.5",
    "prettier": "^2.2.0",
    "semantic-release": "^19.0.2",
    "semantic-release-config-gitmoji": "^1.2.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^5.0.2",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2",
    "yarnhook": "^0.6.0",
    "zustand": "^3.7.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
